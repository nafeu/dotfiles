alias siteconfig="sudo nano /etc/nginx/sites-available/phrakture.com"
alias aliasconfig="nano ~/.bash_aliases"
alias restartnginx="sudo systemctl restart nginx"
alias reloadnginx="sudo systemctl reload nginx"
alias nginxconfig="sudo nano /etc/nginx/nginx.conf"
alias whatalias="cat ~/.bash_aliases"
alias testnginx="sudo nginx -t"
alias howtoenable="echo 'sudo ln -s /etc/nginx/sites-available/your_domain /etc/nginx/sites-enabled/'"
alias howtocreate="echo 'sudo nano /etc/nginx/sites-available/your_domain"
alias logs="cat /var/log/nginx/access.log"
